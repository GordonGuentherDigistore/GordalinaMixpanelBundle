{
    "name": "gordalina/mixpanel-bundle",
    "description": "Symfony bundle for Mixpanel",
    "keywords": ["mixpanel", "bundle", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Gordalina",
            "email": "samuel.gordalina@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/security-http": "^4.3 || ^5.0",
        "mixpanel/mixpanel-php": "~2.6"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.0 || ^6.0"
    },
    "autoload": {
      "psr-0": { "Gordalina\\MixpanelBundle": "" }
    },
    "target-dir": "Gordalina/MixpanelBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
